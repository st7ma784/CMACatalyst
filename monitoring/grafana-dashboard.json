      {
        "id": 4,
        "title": "Advisor Performance",
        "type": "table",
        "targets": [
          {
            "expr": "SELECT u.first_name || ' ' || u.last_name as advisor, COUNT(DISTINCT c.id) as total_cases, COUNT(DISTINCT CASE WHEN c.status = 'closed' THEN c.id END) as closed_cases, ROUND(AVG(EXTRACT(EPOCH FROM (c.updated_at - c.created_at))/86400), 1) as avg_case_duration_days, COUNT(DISTINCT a.id) as total_appointments FROM users u LEFT JOIN cases c ON u.id = c.assigned_advisor_id LEFT JOIN appointments a ON c.id = a.case_id WHERE u.centre_id = $centre_id AND u.role = 'advisor' GROUP BY u.id, u.first_name, u.last_name ORDER BY total_cases DESC",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        },
        "options": {
          "showHeader": true
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Average Appointments per Case",
        "type": "stat",
        "targets": [
          {
            "expr": "SELECT ROUND(AVG(appointment_count), 2) as avg_appointments FROM (SELECT c.id, COUNT(a.id) as appointment_count FROM cases c LEFT JOIN appointments a ON c.id = a.case_id WHERE c.centre_id = $centre_id GROUP BY c.id) subq",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 3
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
