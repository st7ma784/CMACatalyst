{
  "extends": "next/core-web-vitals",
  "rules": {
    "react-hooks/rules-of-hooks": "off",
    "react-hooks/exhaustive-deps": "off",
    "@next/next/no-html-link-for-pages": "off",
    "indent": "off",
    "semi": "off",
    "quotes": "off",
    "no-undef": "off",
    "no-useless-escape": "off",
    "no-unused-vars": "off",
    "react/no-unescaped-entities": "off",
    "react/display-name": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
